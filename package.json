{
  "name": "guido",
  "version": "4.8.2",
  "description": "基于webpack4的一键式开发工具，集成handlebars、雪碧图、SVG Sprite、自定义资源注入等方式",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zuzucheFE/guido.git"
  },
  "bugs": {
    "url": "https://github.com/zuzucheFE/guido/issues"
  },
  "keywords": [
    "guido",
    "webpack",
    "framework",
    "tools",
    "frontend"
  ],
  "files": [
    "bin/",
    "lib/"
  ],
  "scripts": {
    "build:examples": "cd examples && node buildAll.js",
    "test": "jest",
    "travis:test": "npm run test",
    "prettier": "prettier --trailing-comma es5 --single-quote --write 'lib/*/*.js'"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.13.0"
  },
  "bin": {
    "guido": "./bin/guido.js"
  },
  "main": "lib/service.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md}": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/runtime-corejs3": "7.13.10",
    "babel-preset-zuzuche": "2.8.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "address": "1.1.2",
    "assets-webpack-plugin": "7.0.0",
    "autoprefixer": "10.2.5",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "chalk": "4.1.0",
    "css-loader": "5.2.2",
    "del": "6.0.0",
    "detect-port-alt": "1.1.6",
    "eslint": "7.24.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-webpack-plugin": "2.5.3",
    "extend": "3.0.2",
    "file-loader": "6.2.0",
    "filesize": "6.2.5",
    "friendly-errors-webpack-plugin": "1.7.0",
    "glob": "7.1.6",
    "gzip-size": "6.0.0",
    "handlebars": "4.7.7",
    "handlebars-loader": "1.7.1",
    "html-webpack-plugin": "5.3.1",
    "image-webpack-loader": "7.0.1",
    "imagemin-gifsicle": "7.0.0",
    "imagemin-jpegtran": "7.0.0",
    "imagemin-optipng": "8.0.0",
    "imagemin-svgo": "9.0.0",
    "mini-css-extract-plugin": "1.5.0",
    "node-sass": "5.0.0",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "postcss-loader": "5.2.0",
    "postcss-safe-parser": "5.0.2",
    "postcss-sprites": "4.2.1",
    "recursive-readdir": "2.2.2",
    "sass-loader": "11.0.1",
    "strip-ansi": "7.0.0",
    "style-loader": "2.0.0",
    "svg-sprite-loader": "6.0.5",
    "svg-url-loader": "7.1.1",
    "svgo": "2.3.0",
    "svgo-loader": "3.0.0",
    "terser-webpack-plugin": "5.1.1",
    "text-table": "0.2.0",
    "url": "0.11.0",
    "url-loader": "4.1.1",
    "webpack": "5.33.2",
    "webpack-dev-server": "3.11.2",
    "webpack-merge": "5.7.3",
    "webpackbar": "5.0.0-3"
  }
}
